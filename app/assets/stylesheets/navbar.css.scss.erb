#home-navbar {
  height: 50px;
  width: 100%;
  line-height: 60px;
  background-color: #161618;
  font-weight: 600;
  padding-left: 35px;
  padding-right: 35px;

  position: fixed;
  top: 0px;
  z-index: 2;


  .nav-btn {
    font-size: 12px;
    color: #fff;
    border-radius: 4px;
    transition: all 150ms linear;
    -webkit-transition: all 150ms linear;
    -ms-transition: all 150ms linear;
    -moz-transition: all 150ms linear;
     margin-right: 12px;
     margin-left: 4px;
  }

  .nav-btn:hover {
    opacity: 0.8;
    box-shadow: 0px 2px 2px rgba(100,100,100,0.5);
  }

  #nav-signup-btn {
    padding: 7px 16px 7px 16px;
    background-color: #50e3c2;
    color: #FCFCFC;
  }

  #nav-login-btn {

  }

  #nav-logout-btn {
    padding: 7px 21px 7px 21px;
    background-color: #419BF9;
  }

  .left {
    float: left;
  }

  #st{
      display: block;
      position: relative;
      top: 8px;
      right: 10px;
      width: 375px;
      height: 30px;
      padding:6px 15px 6px 30px;
      margin:3px;
      background: url(image_path('search.png')) no-repeat 8px 6px;
      background-color: #e1e1e1;
      border: 1px solid #D4D4D4;
      color: #6f6f6f;
      font-weight: 400;
  }

  #st::-webkit-input-placeholder { /* WebKit browsers */
    color:    #aaa;
  }

  #st:-moz-placeholder { /* Mozilla Firefox 4 to 18 */
     color:    #aaa;
     opacity:  1;
  }

  #st::-moz-placeholder { /* Mozilla Firefox 19+ */
     color:    #aaa;
     opacity:  1;
  }

  #st:-ms-input-placeholder { /* Internet Explorer 10+ */
     color:    #aaa;
  }

  .right {
    position: relative;
    float: right;
    bottom: 5px;

    #arima-icon {
      padding-right: 1em;
      img{
        width: 42px;
      }
    }
  }

}
